        {
            "Name": "ToolTips",
            "Description": "Settings for each ToolTip behaviour.",
            "IsAdvanced": false,
            "Keys": [
                {
                    "Value": false,
                    "Name": "Use12HourTimeFormatting",
                    "Description": "Uses AM/PM instead of 24 hour formatting.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": true,
                    "Name": "OpenIfUnlocked",
                    "Description": "Shows buildings as opened if they are unlocked to you, even if not within opening hours.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": true,
                    "Name": "HoursAboveOpenClosed",
                    "Description": "Shows Hours Above (Open/Closed) instead of below.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": true,
                    "Name": "DontShowHoursForAlwaysAccessible",
                    "Description": "Will not show hours if a building is always accessible.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": false,
                    "Name": "DontShowOpenClosedForAlwaysAccessible",
                    "Description": "Will not show (open/closed) if a building is always accessible.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                }
            ],
            "$version": "v1"
        },

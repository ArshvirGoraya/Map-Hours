{
    "Version": "1.3.0",
    "Sections": [
        {
            "Name": "ToolTips",
            "Description": "Settings for each ToolTip behaviour.",
            "IsAdvanced": false,
            "Keys": [
                {
                    "Value": false,
                    "Name": "Use12HourTimeFormatting",
                    "Description": "Uses AM/PM instead of 24 hour formatting.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": true,
                    "Name": "OpenIfUnlocked",
                    "Description": "Shows buildings as opened if they are unlocked to you, even if not within opening hours.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": true,
                    "Name": "ShowHoursAndOpenClosedInSameLine",
                    "Description": "Instead of seperating into 2 lines will show in the same line.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": false,
                    "Name": "HoursBeforeOpenClosed",
                    "Description": "Shows Hours Before (Open/Closed) instead of After.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": true,
                    "Name": "ShowQualityNextToBuildingName",
                    "Description": "Shows Quality Text/Number next to the building name.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": false,
                    "Name": "QualityNumberBeforeQualityText",
                    "Description": "Shows Quality Number Before Quality Text instead of After.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": false,
                    "Name": "DontShowHoursForAlwaysAccessible",
                    "Description": "Will not show hours if a building is always accessible.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": false,
                    "Name": "DontShowOpenClosedForAlwaysAccessible",
                    "Description": "Will not show (open/closed) if a building is always accessible.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Value": false,
                    "Name": "RevealUndiscoveredBuildings",
                    "Description": "Reveals all undiscovered buildings. Turn off to remove undiscovered buildings. ",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.ToggleKey"
                },
                {
                    "Options": [
                        "Large Font",
                        "Title Font",
                        "Small Font",
                        "Default Font",
                        "Unused"
                    ],
                    "Value": 3,
                    "Name": "ToolTipSize",
                    "Description": "Sets the size of the tooltip text.",
                    "$version": "v1",
                    "$type": "DaggerfallWorkshop.Game.Utility.ModSupport.ModSettings.MultipleChoiceKey"
                }
            ],
            "$version": "v1"
        },
